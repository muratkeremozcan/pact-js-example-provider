require 'rubygems'

Gem.use_gemdeps

version = ">= 0.a"

str = ARGV.first
if str
  str = str.b[/\A_(.*)_\z/, 1]
  if str and Gem::Version.correct?(str)
    version = str
    ARGV.shift
  end
end

if Gem.respond_to?(:activate_bin_path)
load Gem.activate_bin_path('pact_broker-client', 'pactflow', version)
else
gem "pact_broker-client", version
load Gem.bin_path("pact_broker-client", "pactflow", version)
end
